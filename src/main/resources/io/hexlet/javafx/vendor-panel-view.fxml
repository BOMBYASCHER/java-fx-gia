<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="500.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="34.0">
         <children>
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="100.0" text="Name" />
            <TextField fx:id="nameField" focusTraversable="false" prefHeight="25.0" prefWidth="300.0" />
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="100.0" text="Type" />
            <MenuButton fx:id="menuVendorTypeList" focusTraversable="false" mnemonicParsing="false" prefHeight="25.0" prefWidth="300.0" />
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="106.0" text="Director" />
            <TextField fx:id="directorField" focusTraversable="false" prefHeight="25.0" prefWidth="300.0" />
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="106.0" text="Address" />
            <TextField fx:id="addressField" focusTraversable="false" prefHeight="25.0" prefWidth="300.0" />
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="106.0" text="Phone" />
            <TextField fx:id="phoneField" focusTraversable="false" prefHeight="25.0" prefWidth="300.0" />
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="106.0" text="Email" />
            <TextField fx:id="emailField" focusTraversable="false" prefHeight="25.0" prefWidth="300.0" />
            <Label alignment="CENTER" layoutX="10.0" layoutY="260.0" prefHeight="25.0" prefWidth="100.0" text="Rating" textAlignment="CENTER" />
            <TextField fx:id="ratingField" focusTraversable="false" layoutX="10.0" layoutY="285.0" prefHeight="25.0" prefWidth="300.0" />
         </children>
      </VBox>
      <Button fx:id="saveButton" focusTraversable="false" layoutX="110.0" layoutY="415.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Save" />
   </children>
</AnchorPane>
